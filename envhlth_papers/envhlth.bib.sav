  issue              = {2},
  keywords           = {Animals, *Climate Change, Culicidae, *Disease Outbreaks, Europe, Forecasting, Humans, *Insect Vectors, Psychodidae, Ticks, Leishmaniasis, Zika, chikungunya, climate change, dengue, vector-borne diseases},
  language           = {eng},
  location-id        = {fnx244},
