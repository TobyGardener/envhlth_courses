  issue                  = {1},
  keywords               = {Animals, *Climate Change, Communicable Diseases/*epidemiology/*transmission, *Disease Vectors, Humans, *Models, Biological, climate change, emerging disease, public health, vector-borne disease, water-borne disease},
  language               = {eng},
  location-id            = {10.1111/nyas.13950 [doi]},
